        result_compare_ld_and_st32((void *)res.move_int32, (void *)dst0[i].move_int32);
    }
    for (i = 0; i < length; i++) {
        test_mldm4d(src0[i].move_int64, res.move_int64);
        result_compare_ld_and_st((void *)res.move_int64,(void *) src0[i].move_int64);
